import org.jointheleague.graphical.robot.Robot;
public class RobotSquare {
	public static void main(String[] args) {
		int time=26;
		Robot idk=new Robot();
		idk.penDown();
		idk.setSpeed(10);
		idk.setPenColor(0,255,255);
		idk.move(250);
		idk.turn(90);
		idk.move(250);
		idk.turn(90);
		idk.move(250);
		idk.turn(90);
		idk.move(250);
		
		
	}
	
	
	
	
	
	
	
	
	

}
